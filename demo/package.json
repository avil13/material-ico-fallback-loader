{
    "name": "material-fallback-demo",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",

        "tslint:check": "tslint -t verbose -c ./tslint.json --project .",
        "tslint:fix": "tslint -t verbose -c ./tslint.json --fix --project .",
        "format": "prettier --config ./.prettierrc --write '**/*.{vue,ts}'",

        "gh-pages": "node _publish.js",
        "gh": "npm run build && npm run gh-pages"
    },
    "dependencies": {
        "vue": "^2.5.17",
        "vue-class-component": "^6.0.0",
        "vue-property-decorator": "^7.0.0"
    },
    "devDependencies": {
        "@kurosame/tslint-config-vue": "^1.3.0",
        "@vue/cli-plugin-babel": "^3.0.5",
        "@vue/cli-plugin-typescript": "^3.0.5",
        "@vue/cli-service": "^3.0.5",
        "gh-pages": "^2.0.1",
        "node-sass": "^4.9.0",
        "prettier": "^1.15.2",
        "sass-loader": "^7.0.1",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^3.0.0",
        "vue-template-compiler": "^2.5.17"
    }
}
